<p id="notice"><%= notice %></p>

<h1>Cuidads</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuidads.each do |cuidad| %>
      <tr>
        <td><%= cuidad.nombre %></td>
        <td><%= link_to 'Show', cuidad %></td>
        <td><%= link_to 'Edit', edit_cuidad_path(cuidad) %></td>
        <td><%= link_to 'Destroy', cuidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cuidad', new_cuidad_path %>
